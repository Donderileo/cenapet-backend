{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "eventos",
  "info": {
    "name": "Evento"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "titulo": {
      "type": "string"
    },
    "logo": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "data_inicio": {
      "type": "date"
    },
    "local": {
      "type": "string"
    },
    "links": {
      "type": "richtext"
    },
    "data_fim": {
      "type": "date"
    }
  }
}
